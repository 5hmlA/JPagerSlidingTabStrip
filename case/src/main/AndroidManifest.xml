<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jonas.acase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ExpandActivity"
            android:label="@string/title_activity_expand"/>
        <activity
            android:name=".NotExpandActicity"
            android:label="@string/title_activity_not_expand_acticity"/>
        <activity
            android:name=".CustomView"
            android:label="@string/title_activity_custom_view"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>